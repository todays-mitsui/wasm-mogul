{
  "name": "example-with-vitest",
  "scripts": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "test": "vitest run",
    "test-watch": "vitest",
    "test-ui": "vitest --ui",
    "gen:hcm": "hcm 'src/**/*.module.{css,scss,less}'",
    "fmt": "npx @biomejs/biome format --write ./src",
    "precommit": "npm run gen:hcm && npm run fmt"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precommit"
    }
  },
  "type": "module",
  "devDependencies": {
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.0",
    "@solidjs/start": "^1.0.10",
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/user-event": "^14.5.2",
    "@vitest/ui": "^2.1.4",
    "happy-css-modules": "^3.2.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "solid-js": "^1.9.3",
    "typescript": "^5.6.3",
    "vinxi": "^0.4.3",
    "vite": "^5.4.10",
    "vite-plugin-solid": "^2.10.2",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.3.0",
    "vitest": "^2.1.4"
  }
}
