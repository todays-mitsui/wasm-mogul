{
  "name": "example-with-vitest",
  "scripts": {
    "dev": "vinxi dev --port 3033",
    "build": "vinxi build",
    "start": "vinxi start",
    "test": "vitest run",
    "test-watch": "vitest",
    "test-ui": "vitest --ui",
    "gen:hcm": "hcm --localsConvention camelCaseOnly 'src/**/*.module.{css,scss,less}'",
    "gen:hcm:watch": "hcm -w --localsConvention camelCaseOnly 'src/**/*.module.{css,scss,less}'",
    "fmt": "npx @biomejs/biome format ./src",
    "fmt:write": "npx @biomejs/biome format --write ./src",
    "check": "npx @biomejs/biome check ./src",
    "check:write": "npx @biomejs/biome check --write ./src",
    "precommit": "npm run gen:hcm && npm run fmt:write",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "type": "module",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@chromatic-com/storybook": "^3.2.2",
    "@solid-primitives/storage": "^4.2.1",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.1",
    "@solidjs/start": "^1.0.10",
    "@solidjs/testing-library": "^0.8.10",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/user-event": "^14.5.2",
    "@vitest/ui": "^2.1.4",
    "classnames": "^2.5.1",
    "happy-css-modules": "^3.2.0",
    "jsdom": "^25.0.1",
    "solid-js": "^1.9.3",
    "storybook": "^8.4.7",
    "storybook-solidjs": "^1.0.0-beta.6",
    "storybook-solidjs-vite": "^1.0.0-beta.6",
    "typescript": "^5.6.3",
    "vinxi": "^0.4.3",
    "vite": "^5.4.10",
    "vite-plugin-solid": "^2.10.2",
    "vite-plugin-top-level-await": "^1.4.4",
    "vite-plugin-wasm": "^3.3.0",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.4"
  }
}
